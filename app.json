{
  "expo": {
    "name": "TodoApp",
    "slug": "todoapp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "This is a React native Todo App project.\n\nCredits:\n-React Native Expo\n-React Redux\n-React Navigation\n-React Native Elements",
    "githubUrl": "https://github.com/sarpisik/ReactNative-TodoApp"
  }
}